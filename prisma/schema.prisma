generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Card {
  id        String   @id @default(cuid())
  to        String   @db.VarChar(200)
  from      String   @db.VarChar(200)
  message   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
